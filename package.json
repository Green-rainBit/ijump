{
  "name": "ijump",
  "displayName": "IJump（Go Jump gojump） - Go项目接口与实现快速跳转 ",
  "description": "Go语言接口与实现智能跳转工具，支持跨文件分析和精确的代码导航",
  "version": "3.1.4",
  "publisher": "zhengxiaojun",
  "engines": {
    "vscode": "^1.96.0"
  },
  "icon": "resources/icon.png",
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "IJump",
      "properties": {
        "ijump.parserMode": {
          "type": "string",
          "enum": [
            "auto",
            "parser",
            "gopls"
          ],
          "enumDescriptions": [
            "自动选择（优先 parser，不可用时回退到 gopls）",
            "使用自定义 Go 解析器",
            "使用 gopls 语言服务器（需要 Go 扩展）"
          ],
          "default": "auto",
          "description": "选择 Go 代码解析模式"
        },
        "ijump.displayMode": {
          "type": "string",
          "enum": [
            "gutter",
            "codelens",
            "both"
          ],
          "enumDescriptions": [
            "仅显示行号旁边的图标（悬停显示跳转链接）",
            "仅显示 CodeLens（点击直接跳转）",
            "同时显示图标和 CodeLens"
          ],
          "default": "both",
          "description": "选择交互显示模式"
        }
      }
    },
    "commands": [
      {
        "command": "ijump.jumpToImplementation",
        "title": "Jump to Implementation"
      },
      {
        "command": "ijump.jumpToInterface",
        "title": "Jump to Interface Definition"
      },
      {
        "command": "ijump.clearCache",
        "title": "IJump: 清除缓存"
      },
      {
        "command": "ijump.switchMode",
        "title": "IJump: 切换解析模式"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goodends/ijump.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run build-parser",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build-parser": "node ./scripts/build-parser.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.46",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "keywords": [
    "go",
    "golang",
    "interface",
    "navigation",
    "productivity",
    "code-navigation",
    "jump",
    "gojump",
    "go-interface",
    "interface-implementation"
  ]
}